{
  "name": "elm-ts-electron-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "elm-typescript-interop": "elm-typescript-interop renderer/elm/*.elm --output renderer/elm/Main.d.ts",
    "pre-build": "rimraf dist && npm run elm-typescript-interop",
    "build": "npm run pre-build && webpack",
    "build-main": "npm run pre-build && webpack --config webpack.main.ts",
    "dev-server": "npm run pre-build && webpack-dev-server --config webpack.renderer.ts --inline  --content-base /dist",
    "start-dev": "npm run build && electron dist/main.js",
    "start": "electron dist/main.js",
    "start-with-wds": "electron dist/main.js --index http://localhost:${DEV_SERVER_PORT:-8080}/",
    "package": "NODE_ENV=production npm run build && electron-packager dist sample --platform=darwin --arch=x64 --overwrite --electron-version 2.0.2",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "electron": "^2.0.2"
  },
  "devDependencies": {
    "@types/copy-webpack-plugin": "^4.4.1",
    "@types/electron": "^1.6.10",
    "@types/webpack": "^4.4.0",
    "@types/webpack-merge": "^4.1.3",
    "copy-webpack-plugin": "^4.5.1",
    "electron-packager": "^12.1.0",
    "elm": "^0.18.0",
    "elm-format": "^0.7.0-exp",
    "elm-oracle": "^1.1.1",
    "elm-typescript-interop": "0.0.4",
    "elm-webpack-loader": "^4.5.0",
    "file-loader": "^1.1.11",
    "prettier": "^1.13.4",
    "rimraf": "^2.6.2",
    "ts-loader": "^4.3.1",
    "ts-node": "^6.1.0",
    "tslint": "^5.10.0",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.9.1",
    "webpack": "^4.11.1",
    "webpack-cli": "^3.0.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.2",
    "write-file-webpack-plugin": "^4.3.2"
  }
}
